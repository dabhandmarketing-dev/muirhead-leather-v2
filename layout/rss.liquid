<!doctype html>
---
permalink: /sku/rss.xml
eleventyExcludeFromCollections: true
---
{% - if collections['sku'][0] -%}
<XML></XML>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom" xmlns:media="http://search.yahoo.com/mrss/">
  <channel>
    <title>SKUs </title>
    <description>SKUs  Feed</description>
    <link>{{ settings.site.domain }}
    <pubdate>{{ collections['sku'][0].date.toUTCString() }}</pubdate>
    <link href="{{ settings.site.domain }}/sku/rss.xml" rel="self" type="application/rss+xml">
    {% - for post in collections['sku'] | reverse %}
        <item>
            <title>{{ post.data.title }}</title>
            {% if post.data['f_image'] %}
            <content url="{{ settings.site.domain }}{{ post.data['f_image'].url }}" medium="image"></content>
            {% elif post.data['f_main-image'] %}
            <content url="{{ settings.site.domain }}{{ post.data['f_main-image'].url }}" medium="image"></content>
            {% elif post.data['f_thumbnail-image'] %}
            <content url="{{ settings.site.domain }}{{ post.data['f_thumbnail-image'].url }}" medium="image"></content>
            {% endif %}
            <link>{{ settings.site.domain }}{{ post.url | url}}
            <pubdate>{{ post.date.toUTCString() }}</pubdate>
            <guid>{{ settings.site.domain }}{{ post.url | url}}</guid>
            {% if post.data['f_excerpt'] %}
            <description>{{post.data['f_excerpt']}}</description>
            {% elif post.data['f_summary'] %}
            <description>{{post.data['f_summary']}}</description>
            {% elif post.data['f_post-summary'] %}
            <description>{{post.data['f_post-summary']}}</description>
            {% else %}
            <content type="html">{{ post.templateContent }}</content>
            {% endif %}
        </item>
    {% - endfor %}
  </channel>
</rss>
{% endif %}
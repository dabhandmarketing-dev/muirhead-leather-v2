<div class="container cc-full">
              <div li-section="Slideshow" class="seenon2__wrapper">
                <div pb-section="seenon2" class="seenon2__track">
                  {% for block in section.blocks %}
    {% if block.type == "logo" %}
                    <div li-block="Logo" class="seenon2__logo">
                                    <img loading="lazy" src="{{ 'Philips_logo_new.svg' | asset_url }}" alt="" class="seenon2__logo">
                            </div>
            {% endif %}
{% endfor %}
                </div>
                <div class="custom-css w-embed">
                  <style>
[pb-section="seenon2"] {
  position: absolute;
  white-space: nowrap;
  will-change: transform;
  animation: marquee-horizontal 30s linear infinite;
}
@keyframes marquee-horizontal {
  from {
    transform: translateX(0);
  }
  to {
    transform: translateX(-70%);
  }
}
@media screen and (max-width:767px) {
[pb-section="seenon2"] {
  animation: marquee-horizontal 20s linear infinite;
}
@keyframes marquee-horizontal {
  from {
    transform: translateX(0);
  }
  to {
    transform: translateX(-150%);
  }
}
}
</style>
                </div>
              </div>
            </div> {% schema %} {
    "tag": "section",
    "name": "Logo Carousel",
    "class": "section-2 cc-0",
    "blocks": [
        {
            "name": "Logo",
            "type": "logo",
            "settings": null
        }
    ],
    "presets": [
        {
            "name": "Logo Carousel",
            "category": "Liquify",
            "blocks": [
                {
                    "type": "logo"
                }
            ]
        }
    ]
} {% endschema %}
<div class="header_background-image-wrapper">
          {% if section.settings.checkbox_overlay == true %}
<div class="header_overlay-layer"></div>{% endif %}
          <div data-image="desktop" class="hero_bg-image-wrap z-index-1">{% if section.settings.image_desktop_image %}
{{ section.settings.image_desktop_image | image_url: width: 1920 | image_tag: loading: 'eager', class: 'header_background-image' }}
{% else %}
 {{ 'detailed-apparel-1' | placeholder_svg_tag: 'header_background-image' }} 
{% endif %}</div>
          {% unless section.settings.image_mobile_image == blank %}    <div data-image="mobile" li-unless="section.settings.image_mobile_image == blank" class="hero_bg-image-wrap z-index-2 hide-unless-mobile-portrait">
    {% if section.settings.image_mobile_image %}
{{ section.settings.image_mobile_image | image_url: width: 1000 | image_tag: loading: 'eager', class: 'header_background-image' }}
{% else %}
 {{ 'detailed-apparel-1' | placeholder_svg_tag: 'header_background-image' }} 
{% endif %}    </div>
{% endunless %}
        </div>
        <div data-animation="fade-in" class="animation-wrap">
          <div class="padding-global">
            <div class="container-large-14">
              <div class="header_card">
                <div li-content-for-theme-blocks="" style="--text-color--text-primary: {{ section.settings.text_color }}" class="hero1_content">
{% content_for 'blocks' %}
</div>
              </div>
            </div>
          </div>
        </div>
        
        <div class="hidden-css w-embed">
          <style>
  /*
  //
  ///
  //// START Layout
  ///
  //
  */
  /* Vertical */
  [data-vertical-alignment="top"] {
  	justify-content: start;
  }
  [data-vertical-alignment="middle"] {
  	justify-content: center;
  }
  [data-vertical-alignment="bottom"] {
  	justify-content: end;
  }
  /* Vertical */
  [data-vertical-position="top"] {
  	bottom: auto;
    top: 0%;
    transform: translate(-50%, -25%);
  }
  [data-vertical-position="middle"] {
  	 transform: translate(-50%, 50%);
     bottom: 50%;
     top: auto;
  }
  [data-vertical-position="bottom"] {
  	bottom: 0%;
    top: auto;
    transform: translate(-50%, 25%);
  }
  /* Gap */
  [data-gap="0"] {
  	gap: 0;
  }
  [data-gap="tiny"] {
  	gap: 0.75rem;
  }
  [data-gap="small"] {
  	gap: 1.25rem;
  }
  [data-gap="medium"] {
  	gap: 1.5rem;
  }
  [data-gap="large"] {
  	gap: 2.5rem;
  }
  /* Max Width */
  [data-max-width="small"] {
  	max-width: 36rem;
  }
  [data-max-width="medium"] {
  	max-width: 42rem;
  }
  [data-max-width="large"] {
  	max-width: 50rem;
  }
  [data-max-width="huge"] {
  	max-width: 58rem;
  }
  /* Font Color */
  [data-text-color="white"] {
  	color: var(--base-color-neutral--white);
  }
  [data-text-color="dark-green"] {
  	color: var(--base-color-brand--dark-green);
  }
  [data-text-color="green"] {
  	color: var(--base-color-brand--green);
  }
  /* Background Color (Split Image) */
  [data-background-color="white"] {
  	background-color: var(--base-color-neutral--white);
  }
  [data-background-color="light-green"] {
  	background-color: var(--base-color-brand--light-green);
  }
  [data-background-color="green"] {
  	background-color: var(--base-color-brand--green);
  }
  [data-background-color="dark-green"] {
  	background-color: var(--base-color-brand--dark-green);
    color: var(--base-color-neutral--white);
  }
  /* Blur */
  [data-background-blur="on"] {
  	background: var(--base-color-neutral--white-20);;
		backdrop-filter: blur(21px);
    padding: 2.5rem;
    height: auto;
    min-height: auto;
  }
  /* Blur with dark bg */
  [data-background-blur="on"][data-text-color="white"] {
  	background: rgba(0, 0, 0, 0.1);
  }
  .section_full-image:has([data-background-blur="off"]) {
  	justify-content: stretch;
  }
  @media screen and (max-width: 767px) {
    [data-background-blur="on"] {
      margin: 0rem;
      padding-left: var(--_responsive---padding--page-padding);
      padding-right: var(--_responsive---padding--page-padding);
    }
  }
  /*
  //
  ///
  //// END Layout
  ///
  //
  */
</style>
        </div> {% schema %} {
    "tag": "section",
    "name": "Hero 1",
    "class": "section_hero1",
    "blocks": [
        {
            "type": "stars"
        },
        {
            "type": "heading-h1"
        },
        {
            "type": "text"
        },
        {
            "type": "button-group"
        },
        {
            "type": "button-primary"
        },
        {
            "type": "button-secondary"
        },
        {
            "type": "button-inverted"
        },
        {
            "type": "@app"
        }
    ],
    "settings": [
        {
            "id": "checkbox_overlay",
            "type": "checkbox",
            "label": "Overlay",
            "default": true
        },
        {
            "id": "image_desktop_image",
            "type": "image_picker",
            "label": "Desktop Image"
        },
        {
            "id": "image_mobile_image",
            "type": "image_picker",
            "label": "Mobile Image"
        },
        {
            "type": "select",
            "id": "text_color",
            "label": "Text Color",
            "options": [
                {
                    "value": "var(--base-color-neutral--white)",
                    "label": "White"
                },
                {
                    "value": "var(--base-color-brand--dark-green)",
                    "label": "Dark Green"
                },
                {
                    "value": "var(--base-color-brand--green)",
                    "label": "Green"
                }
            ],
            "default": "var(--base-color-neutral--white)"
        }
    ],
    "presets": [
        {
            "name": "Hero 1",
            "category": "Liquify",
            "blocks": [
                {
                    "type": "stars"
                },
                {
                    "type": "heading-h1"
                },
                {
                    "type": "text"
                },
                {
                    "type": "button-group"
                },
                {
                    "type": "button-primary"
                },
                {
                    "type": "button-secondary"
                },
                {
                    "type": "button-inverted"
                }
            ]
        }
    ]
} {% endschema %}
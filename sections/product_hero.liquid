                  <div class="hide-4 w-embed w-script">
                    <script>
  document.addEventListener('DOMContentLoaded', () => {
    const container = document.querySelector('.img-magnifier-container');
    const img = document.getElementById('myimage');
    const glass = document.querySelector('.img-magnifier-glass');
    const zoom = 1.5;
    const glassSize = 150;
    const halfGlass = glassSize / 2;
    glass.style.backgroundImage = `url('${img.src}')`;
    glass.style.backgroundSize = `${img.naturalWidth * zoom}px ${img.naturalHeight * zoom}px`;
    function moveMagnifier(e) {
      e.preventDefault();
      const rect = img.getBoundingClientRect();
      const x = e.clientX - rect.left;
      const y = e.clientY - rect.top;
      // Constrain glass within image bounds
      const boundedX = Math.max(halfGlass, Math.min(rect.width - halfGlass, x));
      const boundedY = Math.max(halfGlass, Math.min(rect.height - halfGlass, y));
      // Position glass centered on cursor
      glass.style.left = `${boundedX - halfGlass}px`;
      glass.style.top = `${boundedY - halfGlass}px`;
      // Align zoomed area exactly with cursor
      glass.style.backgroundPosition = `-${boundedX * zoom - halfGlass}px -${boundedY * zoom - halfGlass}px`;
    }
    container.addEventListener('mousemove', moveMagnifier);
    container.addEventListener('touchmove', (e) => {
      e.preventDefault();
      moveMagnifier(e.touches[0]);
    });
    container.addEventListener('mouseenter', () => glass.style.display = 'block');
    container.addEventListener('mouseleave', () => glass.style.display = 'none');
  });
</script>

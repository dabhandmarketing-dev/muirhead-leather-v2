<div class="padding-global-2 padding-section-small">
          <div class="container-large-8">
            <div class="card_component-2">
              <div id="w-node-b18f2ab6-1f09-2907-d176-c9343d4411a5-029cc97b" class="card_img-wrapper" style="{% if section.settings.left_right == 'right'%}order: 9999;{% endif %}">{% if section.settings.image_image %}
{{ section.settings.image_image | image_url: width: 1000 | image_tag: loading: 'lazy', class: 'card_img-2' }}
{% else %}
<img alt="" src="{{ 'image.svg' | asset_url }}" loading="lazy" class="card_img-2">
{% endif %}</div>
              <div li-content-for-blocks="" class="content_wrapper-2">
{% for block in section.blocks %}
{% case block.type %}
{% when '@app' %}
{% render block %}

                {% when 'headline-small' %}
            <div li-block="Headline small">
                    
                  
                    <div class="text-size-small-18 text-weight-semibold">Small Headline</div>
                    
                
                </div>
                {% when 'headline' %}
            <div li-block="Headline">
                    
                  
                    <h3>
{{ block.settings.text_headline }}
</h3>
                    
                
                </div>
                {% when 'text' %}
            <div li-block="Text">
                    
                  
                    <div> {{ block.settings.richtext_paragraph }} </div>
                    
                
                </div>
                {% when 'buttons' %}
            <div li-block="Buttons">
                    
                  
                    <div class="button-group-7">
                    {% if block.settings.url_button_link %}    <div li-if="block.settings.url_button_link">
    
                      <a href="{{ block.settings.url_button_link }}" class="button-24 w-button"> {{ block.settings.text_button_text }} </a>
                        </div>
{% endif %}
                    {% if block.settings.url_button_link_2 %}    <div li-if="block.settings.url_button_link_2">
    
                      <a href="{{ block.settings.url_button_link_2 }}" class="button-24 is-secondary w-button"> {{ block.settings.text_button_text_2 }} </a>
                        </div>
{% endif %}
                  </div>
                    
                
                </div>
              {% endcase %}
{% endfor %}

    </div>
            </div>
          </div>
        </div> {% schema %} {
    "tag": "section",
    "name": "Flexible Content 2",
    "class": "section_content",
    "blocks": [
        {
            "name": "Headline small",
            "type": "headline-small",
            "settings": null
        },
        {
            "name": "Headline",
            "type": "headline",
            "settings": [
                {
                    "id": "text_headline",
                    "type": "text",
                    "label": "Headline",
                    "default": "Lorem ipsum dolor sit amet, consetetur sadipscing elitr, sed diam nonumy."
                }
            ]
        },
        {
            "name": "Text",
            "type": "text",
            "settings": [
                {
                    "id": "richtext_paragraph",
                    "type": "richtext",
                    "label": "Paragraph",
                    "default": "<p>Lorem ipsum dolor sit amet, consetetur sadipscing elitr, sed diam nonumy eirmod tempor invidunt ut labore et dolore magna aliquyam erat, sed diam voluptua. At vero eos et accusam et justo duo dolores et ea rebum. Stet clita kasd gubergren, no sea takimata sanctus est Lorem ipsum dolor sit amet. Lorem ipsum dolor sit amet.<\/p>"
                }
            ]
        },
        {
            "name": "Buttons",
            "type": "buttons",
            "settings": [
                {
                    "id": "url_button_link",
                    "type": "url",
                    "label": "Button Link"
                },
                {
                    "id": "text_button_text",
                    "type": "text",
                    "label": "Button Text",
                    "default": "Button"
                },
                {
                    "id": "url_button_link_2",
                    "type": "url",
                    "label": "Button Link 2"
                },
                {
                    "id": "text_button_text_2",
                    "type": "text",
                    "label": "Button Text 2",
                    "default": "Button 2"
                }
            ]
        },
        {
            "type": "@app"
        }
    ],
    "settings": [
        {
            "id": "image_image",
            "type": "image_picker",
            "label": "Image"
        },
        {
            "type": "header",
            "content": "image-position"
        },
        {
            "type": "select",
            "id": "image_position",
            "label": "Image position",
            "options": [
                {
                    "value": "left",
                    "label": "Left"
                },
                {
                    "value": "right",
                    "label": "Right"
                }
            ],
            "default": "left"
        }
    ],
    "presets": [
        {
            "name": "Flexible Content 2",
            "category": "Liquify",
            "blocks": [
                {
                    "type": "headline-small"
                },
                {
                    "type": "headline"
                },
                {
                    "type": "text"
                },
                {
                    "type": "buttons"
                }
            ]
        }
    ]
} {% endschema %}
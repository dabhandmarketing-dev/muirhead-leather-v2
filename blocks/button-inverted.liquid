{% when 'button-inverted' %}
            <div li-block="Button Inverted">
                    
                          
                    <div data-animation="stagger" {% if block.settings.large %}data-button='large'{% endif%}>
                            <a href="{{ block.settings.url_button_link }}" class="w-button"> {{ block.settings.text_button_text }} </a>
                          </div>
                    
                          
                    
                    
                        
                </div> {% schema %} {
    "name": "Button Inverted",
    "settings": [
        {
            "id": "url_button_link",
            "type": "url",
            "label": "Button Link"
        },
        {
            "id": "text_button_text",
            "type": "text",
            "label": "Button Text",
            "default": "Button Text"
        },
        {
            "type": "checkbox",
            "id": "large",
            "label": "Large",
            "default": false
        }
    ],
    "presets": [
        {
            "name": "Button Inverted"
        }
    ]
} {% endschema %}